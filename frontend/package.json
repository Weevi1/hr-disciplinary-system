{
  "name": "file-by-fifo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clean": "rm -rf node_modules/.vite && npm run dev",
    "build": "node node_modules/vite/bin/vite.js build",
    "build:fast": "NODE_ENV=production node node_modules/vite/bin/vite.js build --mode=production",
    "build:incremental": "vite build --mode=production --watch=false",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "deploy": "npm run build && firebase deploy",
    "deploy:fast": "npm run build:fast && firebase deploy",
    "deploy:incremental": "npm run build:incremental && firebase deploy"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "firebase": "^11.9.1",
    "lucide-react": "^0.522.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^7.6.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
